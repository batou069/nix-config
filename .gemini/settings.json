{
  "theme": "Dracula",
  "mcpServers": {
    "nixos": {
      "command": "nix",
      "args": [
        "run",
        "github:utensils/mcp-nixos",
        "--"
      ]
    },
    "filesystem": {
      "command": "/etc/profiles/per-user/lf/bin/npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-filesystem",
        "/home/lf/nix",
        "/home/lf/git",
        "/nix"
      ]
    },
    "memory": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-memory"
      ]
    }
  }
}
