{
  "theme": "Dracula",
  "mcpServers": {
    "nixos": {
      "command": "nix",
      "args": ["run", "github:utensils/mcp-nixos", "--"]
    },
    "filesystem": {
      "command": "/etc/profiles/per-user/lf/bin/npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-filesystem",
        "/home/lf/nix",
        "/home/lf/git",
        "/nix"
      ]
    },
    "memory": {
      "command": "bash",
      "args": [
        "-c",
        "npx -y @modelcontextprotocol/server-memory 2>/dev/null"
      ]
    },
    "fetch": {
      "command": "uvx",
      "args": ["mcp-server-fetch"]
    }
  }
}
