{
  "nix.serverSettings": {
    "nixd": {
      "formatting": {
        "command": ["nixpkgs-fmt"]
      },
      "options": {
        "nixos": {
          "expr": "(builtins.getFlake (builtins.toString ./.)).nixosConfigurations.\"lf-nix\".options"
        },
        "home-manager": {
          "expr": "(builtins.getFlake (builtins.toString ./.)).homeConfigurations.\"lf\".options"
        }
      }
    }
  }
}
